#include <iostream>
using namespace std;
int main()
{
int m1, m2, m3, m4, m5;
int total;
float average;
cout << "Enter 5 subject marks: ";
cin >> m1 >> m2 >> m3 >> m4 >> m5;
total = m1 + m2 + m3 + m4 + m5;
average = total/5;
if (average >= 90)
{
cout<<"you pass with grade A";
}
else if (average >= 70)
{
cout<<"you pass grade b";
}
else if (average >= 50)
{
cout<<"you pass with grade c";
}
else if (average >= 35)
{
cout<<"you pass with grade d";
}
else 
{
cout<<"better luck next time";
}
cout << "\nTotal Marks = " << total;
cout << "\nAverage = " << average;
return 0;
}